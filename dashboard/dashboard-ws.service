[Unit]
Description=Dashboard for ECDC server monitoring
After=network.target

[Service]
User=ecdcdashboard
WorkingDirectory=/var/opt/dm_group/dashboard
ExecStart=/usr/bin/scl enable rh-python38 "python -m SimpleHTTPServer 8765"
Restart=always
RestartSec=10
